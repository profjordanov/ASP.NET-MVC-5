@using System.Collections
@using CarDealer.Models.ViewModels;
@model IEnumerable<SupplierVm>
@{
    ViewBag.Title = "All";
}

<h2>All</h2>
<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>number of parts they can offer to supply</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.NumberOfPartsToSupply
            </td>
        </tr>
    }
</table>